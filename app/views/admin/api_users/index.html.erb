<h1>API Users</h1>

<table class="table table-striped" style="width: 2000px;">
  <thead>
    <tr>
      <th scope="col">API Key</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">E-mail</th>
      <th scope="col">Puprose</th>
      <th scope="col">Created</th>
    </tr>
  </thead>
  <tbody>
    <% @api_users.each do |api_user| %>
      <tr>
        <td><%= api_user.api_key %></td>
        <td><%= api_user.first_name %></td>
        <td><%= api_user.last_name %></td>
        <td><%= api_user.email %></td>
        <td><%= api_user.use_description %></td>
        <td><%= api_user.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @api_users %>
